  <step>
    <name>Loan Destination Count</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <sql>select &apos;Loan&apos; as entity_name, loan_status as status, count(*) as destination_count 
from dim_loan
where valid_from &lt;= curdate() and valid_to &gt; curdate()
and loan_account_key &gt; 0
group by entity_name, status
order by entity_name, status

 </sql>
