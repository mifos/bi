  <step>
    <name>Loan Source Count</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>select &apos;Loan&apos; as entity_name, lv.lookup_name as status, count(*) as source_count
from account a
join loan_account la on la.account_id = a.account_id
join account_state ast on ast.account_state_id = a.account_state_id
join lookup_value lv on lv.lookup_id = ast.lookup_id
group by entity_name, status
order by entity_name, status</sql>
